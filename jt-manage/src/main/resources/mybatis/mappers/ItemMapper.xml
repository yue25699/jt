<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jt.mapper.ItemMapper">
	
	<select id="findItemByPage" resultType="Item">
		SELECT * FROM tb_item ORDER BY updated DESC
		LIMIT #{start},#{rows}
		</select>
		</mapper>